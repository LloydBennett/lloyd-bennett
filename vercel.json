{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(png|jpg|jpeg|gif|css|js|ico|svg|woff2?|ttf|eot))",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}

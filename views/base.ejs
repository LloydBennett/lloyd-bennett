<% var page = pageType %>

<%- include('./partials/head', { page: page } ) -%>

<body>
  <main data-page="<%- page %>" class="main <%- page %>">
    
    <% switch(page) {
      case 'project' : %>
        <%- include('./pages/project') %>
        <%  break; %>
      <% case 'about' : %>
        <%- include('./pages/about') -%>
        <% break; %>
      <% default : %>
      <%- include('./pages/home') -%>
    <% } %>

  </main>

  <%- include('./partials/footer', { footer: footer.data }) -%>
</body>



<% 
  let block = slice.items
%>

<section class="section">
  <div class="container">
    <div class="meta meta--margin-bottom">
      <div class="hr"></div>
      <span class="block type--alt-s type--semi-bold type--letter-sp">
        Impact of my work
      </span>
    </div>
    <% block.forEach((item, index) => { %>
      <div class="">
        <div>
          <%- item.icon %>
        </div>
        <h3 class="type--s type--xs-margin">
          <%- item.impact_title %>
        </h3>
        <p class="type--xs">
          <%- item.impact_description %>
        </p>
      </div>
    <% }) %>
  </div>

</section>